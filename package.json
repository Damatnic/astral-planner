{
  "name": "astral-chronos",
  "version": "1.0.2",
  "description": "Astral Chronos - The world's most advanced digital planner with AI-powered productivity features",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:7000": "next dev -p 7000",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__ --testPathIgnorePatterns=e2e",
    "test:integration": "jest --testMatch=\"**/*.integration.test.{js,ts,tsx}\"",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:ci": "npm run test:coverage && npm run test:e2e",
    "test:performance": "k6 run tests/performance/api-load-test.js",
    "test:performance:stress": "k6 run tests/performance/api-load-test.js --env SCENARIO=stress",
    "test:performance:spike": "k6 run tests/performance/api-load-test.js --env SCENARIO=spike",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:performance",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:health": "jest --testMatch=\"**/*health*.test.{js,ts}\"",
    "test:auth": "jest --testMatch=\"**/*auth*.test.{js,ts}\" && playwright test tests/e2e/demo-account.e2e.test.ts",
    "test:report": "npm run test:coverage && npm run test:e2e:report",
    "test:update": "jest --updateSnapshot",
    "test:clear": "jest --clearCache && rm -rf test-results coverage playwright-report",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit --skipLibCheck",
    "type-check:strict": "tsc --noEmit",
    "db:push": "drizzle-kit push:pg",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "tsx scripts/seed.ts",
    "analyze": "ANALYZE=true npm run build",
    "bundle:analyze": "node scripts/analyze-bundle.js analyze",
    "bundle:size": "node scripts/analyze-bundle.js size",
    "bundle:duplicates": "node scripts/analyze-bundle.js duplicates",
    "bundle:report": "node scripts/analyze-bundle.js report",
    "bundle:check": "node scripts/analyze-bundle.js all",
    "perf:lighthouse": "lighthouse http://localhost:3000 --output html --output-path reports/lighthouse.html",
    "perf:audit": "npm run bundle:check && npm run perf:lighthouse",
    "zenith:all": "node scripts/test-master.js full",
    "zenith:quick": "node scripts/test-master.js quick",
    "zenith:ci": "node scripts/test-master.js ci",
    "zenith:pre-commit": "node scripts/test-master.js pre-commit",
    "zenith:performance": "node scripts/test-master.js performance",
    "zenith:parallel": "node scripts/test-master.js full --parallel",
    "zenith:auto-fix": "node scripts/test-master.js full --auto-fix",
    "quality:assurance": "node scripts/quality-assurance.js",
    "performance:test": "node scripts/performance-testing.js",
    "error:detection": "node scripts/error-detection.js",
    "error:check": "node scripts/error-detection.js --pre-commit",
    "error-check": "node scripts/error-detection.js --pre-commit"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.32.2",
