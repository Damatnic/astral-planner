    "jose": "^6.1.0",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "jszip": "^3.10.1",
    "lucide-react": "^0.294.0",
    "next": "^15.0.3",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "openai": "^4.20.1",
    "papaparse": "^5.5.3",
    "pusher": "^5.2.0",
