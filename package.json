    "babel-jest": "^30.1.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.0.2",
    "fast-check": "^4.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.11.3",
